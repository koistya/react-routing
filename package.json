    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "replace": "^0.3.0",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "sinon": "^2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha --recursive --require test/setup",
    "test:watch": "mocha --compilers js:babel-register --recursive --require test/setup --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "build": "node tools/build",
    "prepublish": "node tools/build",
    "deploy": "es deploy docs --repo=kriasoft/universal-router",
    "start": "es start docs"
  }
